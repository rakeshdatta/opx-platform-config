            <entry tag="icmpv6-ns"          cpu-q="5" action="COPY_TO_CPU"/>
            <entry tag="icmpv6-rs"          cpu-q="5" action="COPY_TO_CPU"/>
            <entry tag="icmpv6-mld-query"   cpu-q="4" action="TRAP_TO_CPU"/>
