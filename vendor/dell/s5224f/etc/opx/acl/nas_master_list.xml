            <entry tag="isis-l2"            cpu-q="9"  action="TRAP_TO_CPU"/>
            <entry tag="isis-all"           cpu-q="9"  action="TRAP_TO_CPU"/>
        </table>
    </INGRESS>
    <EGRESS>
    </EGRESS>
</root>
